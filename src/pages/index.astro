---
import Layout from '../layouts/Layout.astro';
import Card from '../components/Card.astro';
import register from '../pages/register.astro';
import login from '../pages/login.astro';
import Launches from '../components/Launches.astro';
---

<Layout title="Ferreteria Forza">
	<main>
		
		<ul role="list" class="link-card-grid">
			<Card
				href="register"
				title="Registrar"
				body="Crea una cuenta de usuario nueva."
			/>
			<Card
				href="login"
				title="Ingresar"
				body="Inicia sesión en tu cuenta de usuario."
			/>
			<Card
				href="https://astro.build/themes/"
				title="Themes"
				body="Explore a galaxy of community-built starter themes."
			/>
			<Card
				href="https://astro.build/chat/"
				title="Community"
				body="Come say hi to our amazing Discord community. ❤️"
			/>
		</ul>
	</main>

	<Launches />
</Layout>
